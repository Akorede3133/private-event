<h1>My Events</h1>

<%= link_to 'Create new Event', new_event_path %>
<ul>
  <% @events.each do |event| %>
    <li><%= event.name %><li>
    <li><%= event.event_date %><li>
    <li><%= event.location %><li>
    <li><%= event.creator.name %><li>
  <% end %>
</ul>

<h2>Attended events<h2>

<ul>
  <% current_user.attended_events.each do |event| %>
  <li><%= event.name %></li>
  <% end %>
</ul>